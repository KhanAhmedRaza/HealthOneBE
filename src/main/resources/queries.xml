<?xml version="1.0" encoding="UTF-8"?>
<queries>
	<queryMap>
		<entry>
			<key>DrugSearchDAO.Drugquery</key>
			<value>
				<sql>
					select d.drugname, d.primaryid, d.caseid from drugs d
					 where UPPER(d.drugname) = ? 
				</sql>
			</value>
			
		</entry>
		<entry>
			<key>UserDAO.insertuserDetails</key>
			<value>
				<sql>
					insert into User_detail(user_id, first_name, last_name, profession, country) values(?, ?, ? ,?,?)
				</sql>
			</value>
			
		</entry>
		
		<entry>
			<key>UserDAO.selectuser</key>
			<value>
				<sql>
					select * from Users where user_id = ?
				</sql>
			</value>
			
		</entry>
		
		<entry>
			<key>UserDAO.insertuser</key>
			<value>
				<sql>
					insert into Users(user_id, password) values(?, ?)
				</sql>
			</value>
			
		</entry>
	</queryMap>
</queries>
